    "    @classmethod\n",
    "    def from_string(cls, string):\n",
    "        return cls(*string.split('-'))\n",
    "    \n",
    "    @staticmethod\n",
    "    def is_valid(string):\n",
    "        return '-' in string\n",
    "    \n",
    "MyClass.is_valid('10-20')"
